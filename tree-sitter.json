{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "plo",
      "camelcase": "Plo",
      "title": "Plo",
      "scope": "source.plo",
      "file-types": [
        "plo"
      ],
      "injection-regex": "^plo$",
      "class-name": "TreeSitterPlo"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPLv3",
    "description": "Plo grammar for tree-sitter",
    "authors": [
      {
        "name": "Keshyu"
      }
    ],
    "links": {
      "repository": "https://github.com/Keshyu/tree-sitter-plo"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}